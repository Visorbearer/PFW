citHeader("To cite PFW in publications use:")

bibentry(
  bibtype      = "Manual",
  title        = "PFW: Tools for Filtering and Processing Data from Project FeederWatch",
  author       =  as.person("Mason Maron"),
  year         = "2025",
  note         = "R package version 0.0.1",
  url          = "https://github.com/Visorbearer/PFW",

  textVersion  =
  paste("Mason Maron (2025).",
        "PFW: Tools for Filtering and Processing Data from Project FeederWatch.",
        "R package version 0.0.1.",
        "https://github.com/Visorbearer/PFW")
)
