citHeader("To cite PFW in publications use:")

bibentry(
  bibtype      = "Manual",
  title        = "PFW: Filtering and Processing Data from Project FeederWatch",
  author       =  as.person("Mason W. Maron"),
  year         = "2025",
  note         = "R package version 0.1.0",
  url          = "https://github.com/ropensci/PFW",

  textVersion  =
  paste("Mason W. Maron (2025).",
        "PFW: Tools for Filtering and Processing Data from Project FeederWatch.",
        "R package version 0.1.0.",
        "https://github.com/ropensci/PFW")
)
